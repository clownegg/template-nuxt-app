{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "parser": "vue-eslint-parser",
  "rules": {
    "vue/multi-word-component-names": "off"
  }
}
